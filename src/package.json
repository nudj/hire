{
  "name": "hire",
  "version": "0.0.1",
  "description": "Nudj hirer website",
  "main": "app/server",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nudj/web.git"
  },
  "author": "Nick Collings",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nudj/web/issues"
  },
  "homepage": "https://github.com/nudj/web#readme",
  "scripts": {
    "start": "yarn run build:dlls && yarn run build:client && node .",
    "dev": "yarn run build:dlls && USE_DEV_SERVER=true DEV_SERVER_PATH=https://hire-wds.local.nudj.co yarn run watch:all",
    "build:client": "webpack --config ./webpack.config.client.js --bail --hide-modules",
    "build:dlls": "webpack --config ./webpack.config.dll.js --bail --hide-modules",
    "watch:all": "webpack-dev-server --config ./webpack.config.client.js & webpack --config ./webpack.config.server.js"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "ignore": [
      "app/server/assets",
      "app/server/build*",
      "app/node_modules",
      "webpack*",
      "@nudj",
      "flow-typed"
    ]
  },
  "dependencies": {
    "@nudj/components": "2.5.0",
    "@nudj/framework": "15.0.0",
    "@nudj/library": "4.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.1",
    "date-fns": "^1.28.3",
    "envkey": "^1.0.0",
    "express": "^4.14.1",
    "flow-bin": "^0.59.0",
    "google-auth-library": "^0.10.0",
    "googleapis": "^21.3.0",
    "hash-generator": "^0.1.0",
    "humanparser": "^1.5.0",
    "intercom-client": "^2.8.7",
    "js-base64": "^2.1.9",
    "linkifyjs": "^2.1.5",
    "lodash": "^4.17.4",
    "mailgun-js": "^0.9.1",
    "node-email-reply-parser": "^0.1.0",
    "papaparse": "^4.3.5",
    "passport": "^0.3.2",
    "passport-auth0": "^0.6.0",
    "passport-google-oauth20": "^1.0.0",
    "prop-types": "^15.6.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-dropzone": "^4.0.0",
    "react-helmet": "^5.0.3",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0",
    "react-textarea-autosize": "^5.1.0",
    "react-virtualized": "^9.18.5",
    "redux": "^3.6.0",
    "route-parser": "^0.0.5",
    "url-search-params": "^0.10.0"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-preset-env": "^1.1.10",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "clean-webpack-plugin": "^0.1.19",
    "dirty-chai": "^1.2.2",
    "eslint-plugin-flowtype": "2.39.1",
    "mocha": "^3.2.0",
    "nock": "^9.0.25",
    "nodemon": "^1.11.0",
    "proxyquire": "^1.8.0",
    "react-hot-loader": "^4.0.0",
    "redux-mock-store": "^1.3.0",
    "sinon": "4.0.1",
    "sinon-chai": "2.14.0",
    "standard": "^10.0.3",
    "start-server-webpack-plugin": "^2.2.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
